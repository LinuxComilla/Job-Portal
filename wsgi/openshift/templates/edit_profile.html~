function createoptionsalary(){
    var min = 0.0;
    var max = 50.0;
    var x = document.getElementById("salarymin");
    for(var i = min; i<=max; i=i+0.5) {
    var option = document.createElement("option");
    option.value = i;
    option.text = 'Rs '+ i+' Lacs/Year';
    x.add(option);
    }}
function editlocation(){
    var x = document.getElementById("countryplace")
    var y = document.getElementById("place")
    for(var i=1;i<=73;i++)
    {
	y.getElementsByTagName("option")[i].disabled=false;
    }
    if(x.value=="US")
    {
	for(var i=1;i<=24;i++)
	{
	    y.getElementsByTagName("option")[i].disabled=true;
	}
    }
    else if(x.value=="India")
    {
	for(var i=25;i<=73;i++)
	{
	    y.getElementsByTagName("option")[i].disabled=true;
	}
    }
    else
    {
	for(var i=1;i<=73;i++)
	{
	    y.getElementsByTagName("option")[i].disabled=true;
	}
    }
}
function chooseoption(){
    var x = document.getElementById("countryplace")
    var y = document.getElementById("place")
    for(var i=1;i<=71;i++)
    {
	y.getElementsByTagName("option")[i].disabled=true;
    }
}
</script>
